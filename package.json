    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.19.1",
    "jest": "24.1.0",
    "prop-types": "15.7.2",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "2.6.1",
    "url-loader": "0.5.9",
    "webpack": "4.30.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2"
  }
