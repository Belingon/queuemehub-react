    "type": "github",
    "url": "https://github.com/Belingon/queuemehub-react"
  },
  "scripts": {
    "start-message": "babel-node scripts/startMessage.js",
    "prestart": "yarn run start-message",
    "start": "concurrently -k -r -s first \"yarn run test:watch\" \"yarn run open:devServer\"",
    "open:devServer": "babel-node scripts/devServer.js",
    "remove-build": "rimraf ./build",
    "clean-build": "yarn run remove-build && mkdir build",
    "prebuild": "yarn run test && yarn run clean-build",
    "build": "babel-node scripts/build.js",
    "test": "jest",
    "test:cover": "yarn run test --coverage",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "axios": "0.17.0",
    "lodash": "4.17.5",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "query-string": "5.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-select": "^2.1.1",
    "recompose": "^0.30.0",
    "redux": "3.7.2",
    "redux-responsive": "4.3.4",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
